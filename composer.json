{
    "name": "ueno-bst/fincode-php-client",
    "version": "20241017.0.1",
    "authors": [
        {
            "name": "t-ueno",
            "email": "t-ueno@bstinc.co.jp"
        }
    ],
    "scripts": {
        "patch": "@composer dump-autoload && php fincode-patch.php && ./vendor/bin/php-cs-fixer fix ./lib",
        "phpstan": "./vendor/bin/phpstan analyze --level 0 ./lib"
    },
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.12",
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.64",
        "nikic/php-parser": "^5.3"
    },
    "autoload": {
        "psr-4": { "OpenAPI\\Fincode\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenAPI\\Fincode\\Test\\" : "test/",
            "OpenAPI\\Fixer\\" : "bin/php/"
        }
    }
}
